type payment_system =
  | Cash 
  | Credit_card 
  | Debit_card 
  | App 

type person_location = {
  mutable _presence: Pbrt.Bitfield.t; (** presence for 2 fields *)
  mutable lat : float;
  mutable lng : float;
}

type person_id2 =
  | X of string
  | Y of int32
  | Z of float

and person = {
  mutable _presence: Pbrt.Bitfield.t; (** presence for 4 fields *)
  mutable id : int64;
  mutable email : string;
  mutable name : string;
  mutable home : person_location option;
  mutable picture : bytes;
  mutable id2 : person_id2 option;
}

let default_payment_system () = (Cash:payment_system)

let default_person_location (): person_location =
{
  _presence=Pbrt.Bitfield.empty;
  lat=0.;
  lng=0.;
}

let default_person_id2 (): person_id2 = X ("")

let default_person (): person =
{
  _presence=Pbrt.Bitfield.empty;
  id=0L;
  email="";
  name="";
  home=None;
  picture=Bytes.create 0;
  id2=None;
}


(** {2 Make functions} *)


let[@inline] has_person_location_lat (self:person_location) : bool = (Pbrt.Bitfield.get self._presence 0)
let[@inline] has_person_location_lng (self:person_location) : bool = (Pbrt.Bitfield.get self._presence 1)

let[@inline] set_person_location_lat (self:person_location) (x:float) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 0); self.lat <- x
let[@inline] set_person_location_lng (self:person_location) (x:float) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 1); self.lng <- x

let copy_person_location (self:person_location) : person_location =
  { self with lat = self.lat }

let make_person_location 
  ?(lat:float option)
  ?(lng:float option)
  () : person_location  =
  let _res = default_person_location () in
  (match lat with
  | None -> ()
  | Some v -> set_person_location_lat _res v);
  (match lng with
  | None -> ()
  | Some v -> set_person_location_lng _res v);
  _res


let[@inline] has_person_id (self:person) : bool = (Pbrt.Bitfield.get self._presence 0)
let[@inline] has_person_email (self:person) : bool = (Pbrt.Bitfield.get self._presence 1)
let[@inline] has_person_name (self:person) : bool = (Pbrt.Bitfield.get self._presence 2)
let[@inline] has_person_picture (self:person) : bool = (Pbrt.Bitfield.get self._presence 3)

let[@inline] set_person_id (self:person) (x:int64) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 0); self.id <- x
let[@inline] set_person_email (self:person) (x:string) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 1); self.email <- x
let[@inline] set_person_name (self:person) (x:string) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 2); self.name <- x
let[@inline] set_person_home (self:person) (x:person_location) : unit =
  self.home <- Some x
let[@inline] set_person_picture (self:person) (x:bytes) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 3); self.picture <- x
let[@inline] set_person_id2 (self:person) (x:person_id2) : unit =
  self.id2 <- Some x

let copy_person (self:person) : person =
  { self with id = self.id }

let make_person 
  ?(id:int64 option)
  ?(email:string option)
  ?(name:string option)
  ?(home:person_location option)
  ?(picture:bytes option)
  ?(id2:person_id2 option)
  () : person  =
  let _res = default_person () in
  (match id with
  | None -> ()
  | Some v -> set_person_id _res v);
  (match email with
  | None -> ()
  | Some v -> set_person_email _res v);
  (match name with
  | None -> ()
  | Some v -> set_person_name _res v);
  (match home with
  | None -> ()
  | Some v -> set_person_home _res v);
  (match picture with
  | None -> ()
  | Some v -> set_person_picture _res v);
  (match id2 with
  | None -> ()
  | Some v -> set_person_id2 _res v);
  _res

