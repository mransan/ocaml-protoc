type payment_system =
  | Cash 
  | Credit_card 
  | Debit_card 
  | App 

type person_location = {
  _presence: Pbrt.Bitfield.t;
  lat : float;
  lng : float;
}

type person_id2 =
  | X of string
  | Y of int32
  | Z of float

and person = {
  _presence: Pbrt.Bitfield.t;
  id : int64;
  email : string;
  name : string;
  home : person_location option;
  picture : bytes;
  id2 : person_id2;
}

let default_payment_system = (Cash:payment_system)

let default_person_location: person_location = 
  {
  _presence=Pbrt.Bitfield.create 2;
  lat=0.;
  lng=0.;
}

let default_person_id2 : person_id2 = X ("")

let default_person: person = 
  {
  _presence=Pbrt.Bitfield.create 4;
  id=0L;
  email="";
  name="";
  home=None;
  picture=Bytes.create 0;
  id2=None;
}


(** {2 Make functions} *)


let rec make_person_location 
  ?(lat:float option)
  ?(lng:float option)
  () : person_location  =
  let _presence = Pbrt.Bitfield.create 2 in
  {
  _presence;
  lat=(match lat with
  | None -> 0.
  | Some v -> (Pbrt.Bitfield.set _presence 0); v);
  lng=(match lng with
  | None -> 0.
  | Some v -> (Pbrt.Bitfield.set _presence 1); v);
}


let rec make_person 
  ?(id:int64 option)
  ?(email:string option)
  ?(name:string option)
  ?(home:person_location option option)
  ?(picture:bytes option)
  ?(id2:person_id2 option)
  () : person  =
  let _presence = Pbrt.Bitfield.create 4 in
  {
  _presence;
  id=(match id with
  | None -> 0L
  | Some v -> (Pbrt.Bitfield.set _presence 0); v);
  email=(match email with
  | None -> ""
  | Some v -> (Pbrt.Bitfield.set _presence 1); v);
  name=(match name with
  | None -> ""
  | Some v -> (Pbrt.Bitfield.set _presence 2); v);
  home=(match home with
  | None -> None
  | Some v -> v);
  picture=(match picture with
  | None -> Bytes.create 0
  | Some v -> (Pbrt.Bitfield.set _presence 3); v);
  id2=(match id2 with
  | None -> None
  | Some v -> v);
}

