
(library
 (name pbrt_services)
 (public_name pbrt_services)
 (wrapped true)
 (synopsis "Runtime library for services generated by ocaml-protoc")
 (libraries pbrt pbrt_yojson yojson))

(rule
  (targets errors.ml errors.mli)
  (deps (:file errors.proto))
  (mode promote)
  (action (run %{bin:ocaml-protoc} --pp --binary --yojson --ml_out . %{file})))
