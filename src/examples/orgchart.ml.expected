type person = {
  mutable _presence: Pbrt.Bitfield.t; (** presence for 2 fields *)
  mutable name : string;
  mutable age : int64;
}

type store = {
  mutable _presence: Pbrt.Bitfield.t; (** presence for 1 fields *)
  mutable address : string;
  mutable employees : person list;
  mutable clients : person list;
}

type company = {
  mutable _presence: Pbrt.Bitfield.t; (** presence for 1 fields *)
  mutable name : string;
  mutable stores : store list;
  mutable subsidiaries : company list;
}

let default_person (): person =
{
  _presence=Pbrt.Bitfield.empty;
  name="";
  age=0L;
}

let default_store (): store =
{
  _presence=Pbrt.Bitfield.empty;
  address="";
  employees=[];
  clients=[];
}

let default_company (): company =
{
  _presence=Pbrt.Bitfield.empty;
  name="";
  stores=[];
  subsidiaries=[];
}


(** {2 Make functions} *)

let[@inline] person_has_name (self:person) : bool = (Pbrt.Bitfield.get self._presence 0)
let[@inline] person_has_age (self:person) : bool = (Pbrt.Bitfield.get self._presence 1)

let[@inline] person_set_name (self:person) (x:string) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 0); self.name <- x
let[@inline] person_set_age (self:person) (x:int64) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 1); self.age <- x

let copy_person (self:person) : person =
  { self with name = self.name }

let make_person 
  ?(name:string option)
  ?(age:int64 option)
  () : person  =
  let _res = default_person () in
  (match name with
  | None -> ()
  | Some v -> person_set_name _res v);
  (match age with
  | None -> ()
  | Some v -> person_set_age _res v);
  _res

let[@inline] store_has_address (self:store) : bool = (Pbrt.Bitfield.get self._presence 0)

let[@inline] store_set_address (self:store) (x:string) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 0); self.address <- x
let[@inline] store_set_employees (self:store) (x:person list) : unit =
  self.employees <- x
let[@inline] store_set_clients (self:store) (x:person list) : unit =
  self.clients <- x

let copy_store (self:store) : store =
  { self with address = self.address }

let make_store 
  ?(address:string option)
  ?(employees=[])
  ?(clients=[])
  () : store  =
  let _res = default_store () in
  (match address with
  | None -> ()
  | Some v -> store_set_address _res v);
  store_set_employees _res employees;
  store_set_clients _res clients;
  _res

let[@inline] company_has_name (self:company) : bool = (Pbrt.Bitfield.get self._presence 0)

let[@inline] company_set_name (self:company) (x:string) : unit =
  self._presence <- (Pbrt.Bitfield.set self._presence 0); self.name <- x
let[@inline] company_set_stores (self:company) (x:store list) : unit =
  self.stores <- x
let[@inline] company_set_subsidiaries (self:company) (x:company list) : unit =
  self.subsidiaries <- x

let copy_company (self:company) : company =
  { self with name = self.name }

let make_company 
  ?(name:string option)
  ?(stores=[])
  ?(subsidiaries=[])
  () : company  =
  let _res = default_company () in
  (match name with
  | None -> ()
  | Some v -> company_set_name _res v);
  company_set_stores _res stores;
  company_set_subsidiaries _res subsidiaries;
  _res

