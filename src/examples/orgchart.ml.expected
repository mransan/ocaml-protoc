(** {2 Protobuf Decoding} *)

let rec decode_pb_person d =
  let v = default_person () in
  let continue__= ref true in
  while !continue__ do
    match Pbrt.Decoder.key d with
    | None -> (
    ); continue__ := false
    | Some (1, Pbrt.Bytes) -> begin
      person_set_name v (Pbrt.Decoder.string d);
    end
    | Some (1, pk) -> 
      Pbrt.Decoder.unexpected_payload_message "person" 1 pk
    | Some (2, Pbrt.Varint) -> begin
      person_set_age v (Pbrt.Decoder.int64_as_zigzag d);
    end
    | Some (2, pk) -> 
      Pbrt.Decoder.unexpected_payload_message "person" 2 pk
    | Some (_, payload_kind) -> Pbrt.Decoder.skip d payload_kind
  done;
  (v : person)

let rec decode_pb_store d =
  let v = default_store () in
  let continue__= ref true in
  while !continue__ do
    match Pbrt.Decoder.key d with
    | None -> (
      (* put lists in the correct order *)
      store_set_clients v (List.rev v.clients);
      store_set_employees v (List.rev v.employees);
    ); continue__ := false
    | Some (1, Pbrt.Bytes) -> begin
      store_set_address v (Pbrt.Decoder.string d);
    end
    | Some (1, pk) -> 
      Pbrt.Decoder.unexpected_payload_message "store" 1 pk
    | Some (2, Pbrt.Bytes) -> begin
      store_set_employees v ((decode_pb_person (Pbrt.Decoder.nested d)) :: v.employees);
    end
    | Some (2, pk) -> 
      Pbrt.Decoder.unexpected_payload_message "store" 2 pk
    | Some (3, Pbrt.Bytes) -> begin
      store_set_clients v ((decode_pb_person (Pbrt.Decoder.nested d)) :: v.clients);
    end
    | Some (3, pk) -> 
      Pbrt.Decoder.unexpected_payload_message "store" 3 pk
    | Some (_, payload_kind) -> Pbrt.Decoder.skip d payload_kind
  done;
  (v : store)

let rec decode_pb_company d =
  let v = default_company () in
  let continue__= ref true in
  while !continue__ do
    match Pbrt.Decoder.key d with
    | None -> (
      (* put lists in the correct order *)
      company_set_subsidiaries v (List.rev v.subsidiaries);
      company_set_stores v (List.rev v.stores);
    ); continue__ := false
    | Some (1, Pbrt.Bytes) -> begin
      company_set_name v (Pbrt.Decoder.string d);
    end
    | Some (1, pk) -> 
      Pbrt.Decoder.unexpected_payload_message "company" 1 pk
    | Some (2, Pbrt.Bytes) -> begin
      company_set_stores v ((decode_pb_store (Pbrt.Decoder.nested d)) :: v.stores);
    end
    | Some (2, pk) -> 
      Pbrt.Decoder.unexpected_payload_message "company" 2 pk
    | Some (3, Pbrt.Bytes) -> begin
      company_set_subsidiaries v ((decode_pb_company (Pbrt.Decoder.nested d)) :: v.subsidiaries);
    end
    | Some (3, pk) -> 
      Pbrt.Decoder.unexpected_payload_message "company" 3 pk
    | Some (_, payload_kind) -> Pbrt.Decoder.skip d payload_kind
  done;
  (v : company)
