(include dune.inc)

(rule
 (alias runtest)
 (action
  (diff dune.inc dune.inc.gen)))

(rule
 (deps
  (:gen-dune ./gen-dune/main.exe))
 (action
  (with-stdout-to
   dune.inc.gen
   (run
    %{gen-dune}
    build_server
    calculator
    example01
    example03
    example04
    example05
    file_server
    orgchart))))
