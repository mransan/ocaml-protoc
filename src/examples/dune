(rule
 (targets example01.ml example01.mli)
 (deps example01.proto)
 (action
  (run ocaml-protoc --binary --pp --ml_out ./ %{deps})))

(test
 (name example01)
 (modules t_example01 example01)
 (package ocaml-protoc)
 (libraries pbrt))

